    "@storybook/html": "^6.2.9",
    "@types/jest": "^26.0.21",
    "@types/puppeteer": "^5.4.3",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
